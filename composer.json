    "description": "Mime-type matching",
    "keywords": [
        "Mime"
    ],
    "homepage": "https://github.com/axypro/mime",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "~3.1"
    },
    "autoload": {
